<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="480.0" prefWidth="400.0"
          style="-fx-background-color: a9a9a9;" xmlns="http://javafx.com/javafx/8.0.121"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainCode.MainController">
    <columnConstraints>
        <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" maxWidth="-Infinity" minWidth="-Infinity"
                           prefWidth="100.0"/>
        <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" minWidth="-Infinity" prefWidth="285.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="-Infinity" minHeight="35.0" prefHeight="35.0" valignment="CENTER" vgrow="ALWAYS"/>
        <RowConstraints maxHeight="-Infinity" minHeight="35.0" prefHeight="35.0" valignment="CENTER" vgrow="ALWAYS"/>
        <RowConstraints maxHeight="5.0" minHeight="5.0" prefHeight="5.0" vgrow="SOMETIMES"/>
        <RowConstraints prefHeight="400.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Button mnemonicParsing="false" onAction="#browseButtonPressed" prefHeight="25.0" prefWidth="85.0" text="Browse"
                GridPane.halignment="CENTER" GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets/>
            </GridPane.margin>
        </Button>
        <TextField fx:id="filePathField" maxWidth="400.0" minWidth="-Infinity" prefHeight="25.0"
                   GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets right="10.0"/>
            </GridPane.margin>
        </TextField>
        <Button layoutX="46.0" layoutY="26.0" mnemonicParsing="false" onAction="#parseButtonPressed" prefHeight="25.0"
                prefWidth="85.0" text="Parse-It" GridPane.halignment="CENTER" GridPane.rowIndex="1"
                GridPane.valignment="CENTER"/>
        <TextArea fx:id="textArea" maxHeight="800.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="405.0"
                  prefWidth="400.0" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS"
                  GridPane.rowIndex="3" GridPane.vgrow="ALWAYS">
            <GridPane.margin>
                <Insets/>
            </GridPane.margin>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
        </TextArea>
        <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <children>
                <Label alignment="CENTER" text="SMS Sent:">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <padding>
                        <Insets left="5.0"/>
                    </padding>
                    <HBox.margin>
                        <Insets right="3.0" top="8.0"/>
                    </HBox.margin>
                </Label>
                <Label prefHeight="17.0" prefWidth="55.0" text="0">
                    <HBox.margin>
                        <Insets right="20.0" top="8.0"/>
                    </HBox.margin>
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
                <Label text="Mins Used:">
                    <HBox.margin>
                        <Insets right="3.0" top="8.0"/>
                    </HBox.margin>
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
                <Label prefHeight="25.0" prefWidth="55.0" text="0">
                    <padding>
                        <Insets top="8.0"/>
                    </padding>
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Label>
            </children>
        </HBox>
    </children>
</GridPane>
